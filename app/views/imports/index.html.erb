<% content_for :menu do %>
  <h1>Importação de Títulos a Receber</h1>
<% end %>


<table id="list" cellpading="0" cellspacing="0" border="0" width="100%">
  <thead>
    <tr>
      <% @doc.xpath("//cadfatre").first.children.each do |node| %>
      <th><%= node.name %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% @doc.xpath("//cadfatre").each do |node| %>
    <tr>
      <% node.children.each do |sub|%>
      <td><%= sub.content %></td>
      <% end %>
    </tr>
    <% end %>
  </tbody>
</table>

